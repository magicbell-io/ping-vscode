{
  "name": "ping-code",
  "version": "0.0.5",
  "private": true,
  "displayName": "ping-code",
  "description": "Ping VSCode extension by Magicbell",
  "author": "MagicBell <bot@magicbell.io> (https://www.magicbell.com)",
  "publisher": "magicbell",
  "contributors": [
    "Stephan Meijer <stephan.meijer@gmail.com>"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "source": "./src/index.tsx",
  "main": "dist/extension.js",
  "engines": {
    "vscode": "^1.73.0",
    "node": "^18.9.0"
  },
  "sideEffects": false,
  "files": [
    "/dist",
    "/src"
  ],
  "homepage": "https://magicbell.com",
  "keywords": [
    "magicbell",
    "notifications",
    "notification center",
    "notification inbox",
    "vscode"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:magicbell-io/magicbell-js.git"
  },
  "bugs": {
    "url": "https://github.com/magicbell-io/magicbell-js/issues"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ping",
          "title": "Ping",
          "icon": "public/bell.svg"
        }
      ]
    },
    "views": {
      "ping": [
        {
          "id": "ping.list",
          "name": "Notifications",
          "type": "webview",
          "initialSize": 4
        }
      ]
    },
    "commands": [],
    "configuration": {
      "type": "object",
      "title": "MagicBell Ping",
      "properties": {
        "ping.apiKey": {
          "type": "string",
          "description": "The MagicBell API Key to use."
        },
        "ping.userHmac": {
          "type": "string",
          "description": "The MagicBell (HMAC) User Key to use."
        },
        "ping.username": {
          "type": "string",
          "description": "Your github username."
        }
      }
    },
    "keybindings": [
      {
        "command": "ping.list.focus",
        "key": "ctrl+alt+p",
        "mac": "ctrl+alt+p"
      }
    ]
  },
  "scripts": {
    "build:dev:app": "webpack --watch --env entry=app --config webpack.config.js --mode development",
    "build:dev:ext": "webpack --watch --env entry=extension --config webpack.config.js --mode development",
    "build:dev": "yarn run build:dev:ext & yarn run build:dev:app",
    "pretest": "yarn run build && yarn run lint",
    "lint": "eslint src --ext ts",
    "clean": "rimraf dist",
    "dev": "code --extensionDevelopmentPath=${PWD}",
    "vscode:prepublish": "yarn build:dev",
    "vscode:package": "yarn vsce package -o dist/ping.vsix --yarn",
    "vscode:publish": "yarn vsce publish -i dist/ping.vsix --yarn",
    "vscode:release": "yarn vscode:package && yarn vscode:publish",
    "vscode:deploy:local": "yarn vscode:package && code --install-extension dist/ping.vsix --force"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/pluralize": "^0.0.29",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/vscode": "^1.73.0",
    "@types/vscode-webview": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "eslint-plugin-react": "^7.32.2",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "vsce": "^2.15.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@preact/signals-core": "^1.2.2",
    "@preact/signals-react": "^1.2.1",
    "@primer/octicons-react": "^17.9.0",
    "@stitches/react": "^1.2.8",
    "@vscode/webview-ui-toolkit": "^1.1.0",
    "global": "^4.4.0",
    "https-browserify": "^1.0.0",
    "magicbell": "1.2.0",
    "path-browserify": "^1.0.1",
    "pluralize": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.0",
    "use-async-effect": "^2.2.7",
    "util": "^0.12.5",
    "vsce": "^2.15.0"
  }
}